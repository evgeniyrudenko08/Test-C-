      <add name="ReportPortal" type="Runtime" />
    <add name="Allure" type="Runtime" /></plugins><stepAssemblies>
      <stepAssembly assembly="ReportPortal.SpecFlowPlugin" />
    <stepAssembly assembly="Allure.SpecFlowPlugin" /></stepAssemblies><!-- For additional details on SpecFlow configuration options see http://go.specflow.org/doc-config --></specFlow></configuration>